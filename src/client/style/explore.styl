.section-explore
	display block
	background-color white
	margin 50px 0px 0px 0px
	padding 0px
	
	.wrapper-filter-a, .wrapper-filter-b
		margin 0px
		padding 0px
		height 37px
		display block
		width 100%
		
		ul
			height 100%
			margin 0px
			align-items center
			padding 0px
			display flex
			align-content center
			justify-content space-around
			list-style none
			
			li
				background-color #E0E0E0
				height 23px
				width 85px
				border-radius 15px
				transition all 0.5s
				
				
				a
					text-decoration none
					width 100%
					height 100%
					display flex
					align-items center
					justify-content center
					span
						margin-right 4px
						
					p
						position relative
						z-index 5
						
					p, span
						height 10px
						text-align center
						line-height 10px
						position relative
						top -1px
						
					.txtSearchBox
						border none
						opacity 0
						position absolute
						width 170px
						padding 0px
						margin 0px
						z-index 6
						transition all 0.5s
			li:hover
				background-color white
				
			.btnFilterActive
				background-color white
				
			.btnSearchActive
				width 200px
				a
					.txtSearchBox
						opacity 1
				
						
	.wrapper-filter-b
		display block
		
	.filters-enter
		opacity 0.1
		
	.filters-enter.filters-enter-active
		opacity 1
		transition opacity 500ms ease-in
	.filters-leave
		opacity 1
	.filters-leave.filters-leave-active
		opacity 1
		transition opacity 300ms ease-in


	.substage-explore
		.wrapper-categories-btn
			display block
			background-color #f4f4f4
			margin 0px
			padding 0px
			position relative
			top 85px
			height 100%
			position fixed
			z-index 5
			width 100%
	
				
			ul
				display flex
				flex-wrap wrap
				width 100%
				margin 15px auto 0px auto
				padding 0px
				list-style none
				
				li
					width 100%
					margin 3px 0px
					padding 0px
					
					a
						text-decoration none
						display flex
						align-content flex-start
						align-items center
						justify-content flex-start
						height 35px
						cursor pointer
						div
							width 40px
							margin 0px 0px 0px 30px
							img
								display block
								height 20px
								
							
						p
							text-align center
							margin 0px
							
						span
							margin-top 3px
							margin-left 5%
							display none
							
				li:hover
					background-color white
					a span
						display block
						
				.btnCategorySelected
					background-color white

.wrapper-thumbnails
	display flex
	flex-direction column
	justify-content center
	align-content center
	margin 120px auto 0px auto
	width auto
	.thumbnail
		display block
		width 100%
		height 260px
		overflow hidden
		.thumb-image
			position relative
			z-index 1
			display block
			width 100%
			height 100%
			margin 0px
			padding 0px
			overflow hidden
			
			
			img
				margin 0px
				padding 0px
				display block
				width 100%
				
			
		.mask
			position relative
			z-index 2
			display block
			margin 0px
			padding 0px
			top -260px		
			.transparent-mask-content
				display block
				position relative
				z-index 3	
				height 260px
				width 100%
				margin 0
				opacity 1
				transition all 1s
				overflow hidden
				border-radius 10px
				background: -moz-linear-gradient(top,  rgba(252,252,252,0) 0%, rgba(249,249,249,0) 1%, rgba(0,0,0,0.3) 100%);
				background: -webkit-linear-gradient(top,  rgba(252,252,252,0) 0%,rgba(249,249,249,0) 1%,rgba(0,0,0,0.3) 100%);
				background: linear-gradient(to bottom,  rgba(252,252,252,0) 0%,rgba(249,249,249,0) 1%,rgba(0,0,0,0.3) 100%);
				
				
				.wrapper-price
					line-height 30px
					background-color #00aeb5
					opacity 0.7
					position relative
					top 13px
					left 0px
					height 45px
					width 120px
					display flex
					flex-wrap wrap
					align-items center
					align-content center
					padding 0px 7px
					
					.price
						margin 0px
						padding 0px
						text-align left
						height 26px
					p
						margin 0px
						padding 0px
						height 13px
						line-height 13px
						text-align right
					.price, p
						width 100%
					
				.background-ico-play
					background-color white
					opacity 0.3
					width 50px
					height 50px
					border-radius 50%
					margin 60px auto 0px auto
					padding 0px
					
			
				.ico-play
					margin -50px auto 0px auto
					display block
					width 50px
					height 50px
					line-height 50px
					text-align center
					font-size 2.5em
					border-radius 50%
					color white
											
				h3, h4
					display block
					position relative
					top 55px
					margin 0px 4% 0px 0px
					padding 0px
					text-align right
					
				h3
					text-transform uppercase
					
				h4
					text-transform capitalize

				

			.blur-mask-content
				display block
				position relative
				top -250px
				z-index 2
				height 100%
				margin 0px
				padding 0px
				opacity 0
				transition all 1s
				width auto
					
				
				.wrapper-blur-mask-btn
					display flex
					justify-content space-between
					margin 13px 0px 13px 0px
					padding 0px 25px 10px 25px
					
				h1, .thumb-text
					margin 0px
					padding 0px 25px
					
				.thumb-description
					margin 6px 0px
					height 60px

				
				.wrapper-social-interaction-data
					list-style none
					margin 0px 0px 0px 0px
					padding 0px
					padding 0px 30px
					li
						display inline-block
						margin-right 4px
						
						a, p
							display inline-block
							
						a
							text-decoration none
							margin-right 3px
							.icos
								position relative
								top 2px
								
							.ico-heart-active
								display block
								color #DC5F5F
							
							.icon-heart-outline, .ico-heart-active
								position relative
								top 4px
								
							 
					.floating-wrapper-data
						float right
						
					.comments-info
						margin-left 5px


	.thumbnail:hover
		.mask
			.transparent-mask-content
				opacity 0
			
			.blur-mask-content
				opacity 1
				z-index 4
		.thumb-image
			-webkit-filter: blur(18px);
			-moz-filter: blur(18px);
			-o-filter: blur(18px);
			-ms-filter: blur(18px);
			filter blur(18px)
				

