		flex-direction row
		flex-wrap wrap
		width 712px
		margin 130px auto 0px auto
		justify-content flex-start
		.thumbnail
			width 346px
			height 283px
			margin 0px 5px 10px 5px
			.mask
				top -283px
				.transparent-mask-content
					height 283px
					h3, h4
						top 189px
						
				.blur-mask-content
					top -275px
					.thumb-service-title
						text-transform uppercase
					.wrapper-social-interaction-data
						margin 17px 0px 0px 0px

